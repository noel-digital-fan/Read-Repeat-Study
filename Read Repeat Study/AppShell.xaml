<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Read_Repeat_Study.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:Read_Repeat_Study.Pages"
    Title="Read_Repeat_Study">

    <!-- Make sure this ShellContent points to HomePage -->
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate pages:HomePage}"
        Route="home" />

    <!-- Your other routes -->
    <ShellContent
        Route="AddEditFlagPage"
        ContentTemplate="{DataTemplate pages:AddEditFlagPage}"
        IsVisible="False" />
</Shell>
